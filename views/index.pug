doctype html
html
	head
		title= title
		// css per index - login - register
		link(href='/stylesheets/style.css', rel="stylesheet", type="text/css")
	body
		script.
			function move(dove) {
				switch(dove){
					case "login":
						window.location.href = '/login'
						break;
					case "register":
						window.location.href = '/register'
						break;
					default:
						alert('redirect error')
				}
			}
